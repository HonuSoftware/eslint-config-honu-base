{
  "name": "eslint-config-honu-base",
  "version": "0.4.0",
  "description": "ESLint base configuration make extension simpler",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint rules *.js",
    "check-rules": "not-in-install && (eslint-find-rules --unused && npm run --silent lint) || in-install",
    "test": "./node_modules/.bin/mocha --recursive"
  },
  "author": "Caleb G. Morris <caleb.morris.g@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/HonuSoftware/eslint-config-honu-base"
  },
  "license": "MIT",
  "dependencies": {
    "eslint": "^3.0.1",
    "eslint-find-rules": "^1.11.0",
    "eslint-plugin-react": "^5.2.2",
    "in-publish": "^2.0.0"
  },
  "peerDependencies": {
    "eslint": "^3.0.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint-code-review": "^0.1.1",
    "eslint-plugin-import": "^1.14.0",
    "mocha": "^2.5.3"
  }
}
